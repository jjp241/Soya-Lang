# default value as argument
def silniaRekurencyjna(n = 5) -> int {
    if (n == 1) {
        return 1;
    } 
    else { 
    	return n * silniaRekurencyjna(n - 1);
    }
}

print(silniaRekurencyjna()); # 120
print(silniaRekurencyjna(3)); # 6

def silniaIteracyjna(n = int) -> int {
    res = 1;
    for (i = 2 to n) {
        res = res * i;
    }
    return res;
}